@using TravelWebApp.Models.Classes;
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestLayout.cshtml";
}

<!DOCTYPE HTML>
<html>
<body>
    <div class="content">
        <div class="wrap">
            <div class="about">
                <h3>Hakkımızda</h3>
                @foreach (var x in Model)
                {
                    <img src="@x.ImageURL" alt="" />
                    <p>@x.Explanation</p>
                }
            </div>
        </div>
    </div>
</body>
</html>

