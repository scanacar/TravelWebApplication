@using TravelWebApp.Models.Classes
@model List<Blog>

<div class="padding1">
    <h4>En Beğenilen Yerler</h4>
    <div class="box-left">
        @foreach (var x in Model)
        {
            <div class="bot-img">
                <img src="@x.BlogImageURL" alt=""  style="width:125px; height:75px"/>
            </div>
            <div class="bot-desc">
                <p>@x.Explanation.Substring(0,75)...</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("Partial5", "Default")
</div>
